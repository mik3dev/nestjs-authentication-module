{
  "name": "nestjs-authentication-module",
  "version": "0.1.1-0",
  "description": "Authentication module for NestJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist",
    "check-types": "tsc --noemit",
    "check-code": "npm run lint && npm run check-types",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepare": "husky"
  },
  "keywords": [
    "nestjs",
    "authentication",
    "auth",
    "jwt",
    "passport",
    "strategy",
    "nestjs-authentication-module"
  ],
  "author": "mik3dev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mik3dev/nestjs-authentication-module.git"
  },
  "homepage": "https://github.com/mik3dev/nestjs-authentication-module#readme",
  "bugs": {
    "url": "https://github.com/mik3dev/nestjs-authentication-module/issues"
  },
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.5.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.0",
    "@types/node-jose": "^1.1.13",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jwks-rsa": "^3.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "rxjs": "^7.8.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "jwks-rsa": "^3.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "rxjs": "^7.8.2"
  },
  "dependencies": {
    "node-jose": "^2.2.0"
  }
